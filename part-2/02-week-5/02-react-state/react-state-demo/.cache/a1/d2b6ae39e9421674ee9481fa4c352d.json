mn":18},"source":"components/Card.js","original":{"line":42,"column":19},"name":"_default"},{"generated":{"line":46,"column":26},"source":"components/Card.js","original":{"line":42,"column":19}}],"sources":{"components/Card.js":"import React, {useState} from 'react'\n\n//define the function\n//JSX - allows us to write html-like tags in our javascript\nfunction Card(props) {\n    const {name, genre, id, getAllBands} = props\n    console.log('id', id)\n\n    const deleteBand = (id) => {\n        fetch(`http://localhost:8080/bands/${id}`, {\n          method: \"DELETE\",\n          headers: {\n            'Content-Type': 'application/json'\n          }\n        })\n          .then(response => {\n            if (!response.ok) {\n              throw new Error('Network response was not ok');\n            }\n            console.log('Band deleted successfully');\n            getAllBands()\n          })\n          .catch(error => console.error(error));\n    };\n\n    const handleClick = () => {\n        deleteBand(id)\n    }\n\n    return (\n        <div className=\"card\">\n            <p>This band's name is {name}</p>\n            <p>The genre is called {genre}</p>\n            <button onClick={handleClick}>delete</button>\n            <button>update</button>\n        </div>\n    )\n}\n\n\n//export the function - export default \nexport default Card"},"lineCount":null}},"error":null,"hash":"47c516e9f9fefdf08b454a59cab3097a","cacheData":{"env":{}}}